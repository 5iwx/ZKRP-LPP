page{
	background-color: #f4f4f4;
	padding-bottom: 48rpx;
}
map{
	width: 100%;
	height: 1080rpx;
}
.fixed_model{
	position: relative;
	background: #FFFFFF;
	box-shadow: 0 4rpx 32rpx -4rpx rgba(0,0,0,0.14), 0 4rpx 4rpx -4rpx rgba(0,0,0,0.13);
	border-radius: 16rpx;
	padding: 30rpx 40rpx;
	width: 620rpx;
	height: 128rpx;
	margin: -84rpx auto 0;
}
.fixed_model .title{
	font-size: 32rpx;
	color: var(--mb-color);
}
.fixed_model .in{
	font-size: 28rpx;
	margin-top: 5rpx;
	color: var(--mb-color);
	font-weight: 300;
}
.fixed_model .btn{
	position: absolute;
	background: #F0F0F0;
	width: 630rpx;
	height: 50rpx;
	right: 30rpx;
	top: 100rpx;
	border-radius: 0%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn image{
	width: 50%;
	height: 50%;
	margin: auto;
}
.commit_model{
	background: #32CD32;
	box-shadow: 0 4rpx 30rpx -4rpx rgba(0,0,0,0.14), 0 4rpx 4rpx -4rpx rgba(0,0,0,0.13);
	border-radius: 20rpx;
	width: 100rpx;
	height: 48rpx;
	display: flex;
	padding: 10rpx;
	position: absolute;
    top: 30rpx;
    right: 30rpx;
}
.commit_model view{
  font-size: 24rpx;
	color: white;
	letter-spacing: 0;
	text-align: right;
	font-weight: 600;
	margin: auto;
}
.info_model{
	background-color: #FFFFFF;
	width: 690rpx;
	margin: 32rpx auto;
	border-radius: 16rpx;
}
.info_model .info_item{
	padding: 40rpx;
	min-height: 48rpx;
	display: flex;
}
.info_model .info_item view{
	font-size: 32rpx;
	color: var(--lb-color);
	width: 35%;
}
.info_model .info_item view+view{
	font-size: 32rpx;
	color: #000000;
	width: 65%;
	text-align: right;
}
.info_model .info_item+.info_item{
	box-shadow: inset 0 2rpx 0 0 rgba(224,224,224,0.50);
}